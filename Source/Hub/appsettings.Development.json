{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Path": "logs/hub.log"
      }
    ]
  },
  "Watchers": [
    {
      "Name": "Daemon1",
      "Address": "ws://localhost:5001"
    },
    {
      "Name": "Daemon2",
      "Address": "ws://localhost:5002"
    }
  ]
}
