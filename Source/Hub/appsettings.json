{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Path": "logs/hub.log"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "Watcher.Hub"
    }
  },
  "Watchers": [
    {
      "Name": "Daemon1",
      "Address": "ws://localhost:5001"
    },
    {
      "Name": "Daemon2",
      "Address": "ws://localhost:5002"
    }
  ]
}
