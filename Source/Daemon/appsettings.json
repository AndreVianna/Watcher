{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Path": "logs/watcher.log"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "Watcher.Daemon"
    }
  },
  "Hub": {
    "BaseAddress": "https://localhost:5000/",
    "MessageBufferSize": 1024
  }
}
