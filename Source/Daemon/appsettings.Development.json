{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Path": "logs/watcher.log"
      }
    ]
  },
  "Watcher": {
    "BaseAddress": "ws://127.0.0.1:5000/",
    "MessageBufferSize": 1024
  },
  "Hub": {
    "BaseAddress": "ws://127.0.0.1:7000/",
    "MessageBufferSize": 1024
  }
}